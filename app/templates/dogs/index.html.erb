<html lang="en">
  <%= render_partial 'head' %>
  <body>
    <h1><%= @title %></h1>
    <p><a href="/dogs/new">Create a new dog</a></p>
    <ul>
      <% @dogs.each do |dog| %>
        <li>
          <a href="/dogs/<%= dog.id %>">
            <%= "#{dog.id} - #{dog.name}" %>
          </a>
        </li>
      <% end %>
    </ul>
  </body>
</html>